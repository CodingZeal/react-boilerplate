{
  "name": "zeal-react-boilerplate",
  "version": "0.0.1",
  "description": "Zeal's React/Redux boilerplate setup",
  "main": "index.js",
  "scripts": {
    "build": "NODE_ENV=production ./node_modules/.bin/webpack",
    "dev": "webpack-dev-server --devtool eval --progress --colors --hot --history-api-fallback",
    "lint": "NODE_ENV=production eslint 'client/**/*.js' --max-warnings 0 && sass-lint -v --max-warnings 0",
    "postinstall": "npm prune && npm run build",
    "starterapp": "webpack-dev-server --config webpack/starterApp.js --devtool eval --progress --colors --hot --history-api-fallback",
    "test": "NODE_ENV=test ./node_modules/.bin/mocha-webpack",
    "test:debug": "NODE_ENV=test-debug webpack-dev-server --hot --inline",
    "test:watch": "npm run test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/CodingZeal/react-boilerplate.git"
  },
  "homepage": "https://github.com/CodingZeal/react-boilerplate",
  "bugs": {
    "url": "https://github.com/CodingZeal/react-boilerplate/issues"
  },
  "author": "Randy Coulman <randy@codingzeal.com> (http://codingzeal.com)",
  "license": "MIT",
  "engines": {
    "node": "6.2.0",
    "npm": "3.8.9"
  },
  "private": true,
  "dependencies": {
    "babel-core": "^6.9.1",
    "babel-eslint": "^6.0.4",
    "babel-loader": "^6.2.4",
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.9.0",
    "babel-preset-react": "^6.5.0",
    "babel-preset-stage-0": "^6.5.0",
    "chai": "^3.4.1",
    "chai-as-promised": "^5.3.0",
    "classnames": "^2.2.5",
    "css-loader": "^0.23.1",
    "es6-promise": "^3.0.2",
    "eslint": "^2.10.2",
    "eslint-config-zeal": "^0.7.0",
    "eslint-import-resolver-webpack": "^0.3.0",
    "eslint-loader": "^1.3.0",
    "eslint-plugin-import": "^1.8.0",
    "eslint-plugin-react": "^5.1.1",
    "expect-react-shallow": "^1.1.0",
    "exports-loader": "^0.6.2",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.8.5",
    "flux-standard-action": "^0.6.0",
    "foundation-apps": "^1.2.0",
    "history": "^1.17.0",
    "humps": "^1.0.0",
    "imports-loader": "^0.6.5",
    "json-loader": "^0.5.4",
    "mocha": "^2.3.4",
    "mocha-loader": "^0.7.1",
    "mocha-webpack": "^0.4.0",
    "node-sass": "^3.7.0",
    "ramda": "^0.21.0",
    "react": "^0.14.5",
    "react-addons-test-utils": "^0.14.5",
    "react-css-modules": "^3.6.3",
    "react-dom": "^0.14.5",
    "react-hot-loader": "^1.3.0",
    "react-redux": "=4.4.3",
    "react-router": "^2.4.0",
    "react-router-named-routes": "0.0.9",
    "react-router-redux": "^4.0.4",
    "redux": "^3.1.0",
    "redux-api-middleware": "^1.0.0-beta3",
    "redux-form": "^4.1.2",
    "redux-thunk": "^1.0.3",
    "sass-lint": "^1.7.0",
    "sass-loader": "^3.1.2",
    "sasslint-webpack-plugin": "^1.0.2",
    "sinon": "^2.0.0-pre",
    "sinon-chai": "sjmulder/sinon-chai#pr/sinon-2.0.0-pre",
    "source-map-support": "^0.4.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.12.9",
    "webpack-dev-server": "1.14.0",
    "whatwg-fetch": "^0.11.0"
  },
  "babel": {
    "presets": [
      "react",
      "es2015",
      "stage-0"
    ],
    "plugins": [
      "transform-decorators-legacy"
    ]
  }
}
